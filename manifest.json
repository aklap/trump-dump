{
"manifest_version": 2,
    "name": "Trump Dump",
    "description": "Relieve Trump fatigue online by replacing text/images with puppies",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "css": [
                "stylesheet.css"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
      "web_accessible_resources": ["stylesheet.css"]
}